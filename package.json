{
  "name": "hacka",
  "version": "0.0.0",
  "scripts": {
    "build": "./node_modules/.bin/ts-node build.ts",
    "lint": "eslint . --ext .ts",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:con": "concurrently \"nodemon\" \"cd client && nodemon\"",
    "start:dev": "nodemon",
    "start:pro": "ts-node -r tsconfig-paths/register --files",
    "test": "nodemon --config ./spec/nodemon.json",
    "build:test": "tsc --build tsconfig.json && npm run path",
    "build:tsc": "tsc --project tsconfig.json && npm run path",
    "build:webpack": "webpack --config webpack.config.js",
    "path": "tscpaths -p ./tsconfig.json -s ./src -o ./dist"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "prettier"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "no-console": 1,
      "no-extra-boolean-cast": 0,
      "prettier/prettier": "error",
      "arrow-body-style": "off",
      "prefer-arrow-callback": "off",
      "@typescript-eslint/restrict-plus-operands": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-assignment": 0
    }
  },
  "eslintIgnore": [
    "src/public/",
    "build.ts"
  ],
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "autoprefixer": "^10.2.5",
    "bcryptjs": "^2.4.3",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "D": "^1.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.4",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "notistack": "^2.0.3",
    "postcss": "^8.2.13",
    "react": "^17.0.2",
    "react-daum-postcode": "^3.0.1",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "tailwindcss": "^2.1.2",
    "tslint-react": "^5.0.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/command-line-args": "^5.0.0",
    "@types/connect-flash": "0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/express-flash": "0.0.2",
    "@types/express-session": "^1.17.3",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.10",
    "@types/jasmine": "^3.6.9",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.2",
    "@types/mysql": "^2.15.18",
    "@types/node": "^14.14.37",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "aws-sdk": "^2.886.0",
    "camelcase-keys-deep": "^0.1.0",
    "class-transformer": "^0.4.0",
    "concurrently": "^6.2.0",
    "connect-flash": "^0.1.1",
    "ejs": "^3.1.6",
    "eslint": "^7.32.0",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "express-validator": "^6.10.0",
    "find": "^0.3.0",
    "firebase-admin": "^10.0.0",
    "form-data": "^4.0.0",
    "fs-extra": "^9.1.0",
    "http-proxy-middleware": "^2.0.1",
    "jasmine": "^3.7.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "prettier": "^2.7.1",
    "randomstring": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "^4.2.4",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
